            <stroke android:color="@color/white"
                android:width="1dp"/>
            <corners
                android:topLeftRadius="200dp"
                android:topRightRadius="200dp"
                android:bottomRightRadius="200dp"
                android:bottomLeftRadius="200dp"/>
        </shape>
    </item>

</layer-list>